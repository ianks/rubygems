# frozen_string_literal: true

require "test_helper"

class <%= config[:constant_name] %>Test < Test::Unit::TestCase
  test "VERSION" do
    assert do
      ::<%= config[:constant_name] %>.const_defined?(:VERSION)
    end
  end

  test "something useful" do
    <%- if config[:rust] -%>
    assert_equal "Hello from Rust, sea creature!", <%= config[:constant_name] %>.hello("fellow rustacean")
    <%- else -%>
    assert false
    <%- end -%>
  end
end
